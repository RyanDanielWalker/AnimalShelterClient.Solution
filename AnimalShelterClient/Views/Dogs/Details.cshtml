@{
  ViewData["Title"] = "Details";
}

<h1>Info for @Model.Name:</h1>
<ul>
  <li>ID Number: @Model.DogId</li>
  <li>Gender: @Model.Gender</li>
  <li>Age: @Model.Age</li>
  <li>Description: @Model.Description</li>
</ul>
<br>

<a href="/Dogs/Edit/@(Model.DogId)">
  <p>Edit @Model.Name</p>
</a>
<a href="/Dogs">
  <p>Return to All Dogs</p>
</a>
